{
  "MyMusic": {
    "Server": {
      "BaseUrl": "http://localhost:5000/api"
    },
    "Device": {
      "Name": "My Laptop",
      "Icon": null,
      "Color": null,
      "NamingTemplate": null
    },
    "Repository": {
      "Path": "",
      "ExcludePatterns": [
        "**/.*",
        "**/Thumbs.db",
